<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Workshop_main">


    <Button
        android:id="@+id/logout"
        android:layout_width="353dp"
        android:layout_height="62dp"
        android:layout_marginTop="652dp"
        android:background="#009688"
        android:onClick="logout"
        android:text="logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ProfileW"
        android:layout_width="353dp"
        android:layout_height="62dp"
        android:layout_marginTop="464dp"
        android:background="#009688"
        android:onClick="profileW"
        android:text="Profile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="191dp"
        android:layout_height="172dp"
        android:background="#00009688"
        android:src="@drawable/craftsman"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.572"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.191" />

    <Button
        android:id="@+id/checkRequests"
        android:layout_width="353dp"
        android:layout_height="62dp"
        android:background="#009688"
        android:text="Check Requests"
        app:layout_constraintBottom_toTopOf="@+id/ProfileW"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.915" />

    <Button
        android:id="@+id/button4"
        android:layout_width="349dp"
        android:layout_height="58dp"
        android:background="#009688"
        android:onClick="support"
        android:text="Support"
        app:layout_constraintBottom_toTopOf="@+id/logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ProfileW" />
</androidx.constraintlayout.widget.ConstraintLayout>