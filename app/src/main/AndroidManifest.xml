<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobilecarworkshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".support"></activity>
        <activity android:name=".RequestPage" />
        <activity android:name=".FirstPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Profile"
            android:parentActivityName=".Customer" />
        <activity
            android:name=".Login"
            android:parentActivityName=".MainPage" />
        <activity
            android:name=".Regeister"
            android:parentActivityName=".MainPage" />
        <activity android:name=".Customer" />
        <activity
            android:name=".MainPage"
            android:parentActivityName=".FirstPage" />
    </application>

</manifest>